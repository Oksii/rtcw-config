loper1
{
	spawn
	{
		wait 320
		playanim 91 102 looping forever rate 15 //walking
		accum 0 set 1
		wait 180
		trigger loper1 run
	}

	trigger run
	{
		trigger loper1 run1_if
		trigger loper1 run2_if
		trigger loper1 run3_if
		trigger loper1 run4_if
	}

	trigger run1_if
	{
		accum 0 abort_if_not_equal 1
		trigger loper1 run1
	}
	
	trigger run2_if
	{
		accum 0 abort_if_not_equal 2
		trigger loper1 run2
	}
	
	trigger run3_if
	{
		accum 0 abort_if_not_equal 3
		trigger loper1 run3
	}
	
	trigger run4_if
	{
		accum 0 abort_if_not_equal 4
		trigger loper1 run4
	}

	trigger run1
	{
		accum 0 set 2
		gotomarker loper1_spline1 85 
		faceangles 0 180 0 gototime
		gotomarker loper1_spline1_ctrl 85 
		faceangles 0 120 0 gototime
		trigger loper1 run
	}

	trigger run2
	{
		accum 0 set 3
		gotomarker loper1_spline2 85 
		faceangles 0 90 0 gototime
		gotomarker loper1_spline2_ctrl 85 
		faceangles 0 30 0 gototime
		trigger loper1 run
	}

	trigger run3
	{
		accum 0 set 4
		gotomarker loper1_spline3 85  
		faceangles 0 0 0 gototime
		gotomarker loper1_spline3_ctrl 85 
		faceangles 0 300 0 gototime
		trigger loper1 run
	}

	trigger run4
	{
		accum 0 set 1
		gotomarker loper1_spline4 85 
		faceangles 0 270 0 gototime
		gotomarker loper1_spline4_ctrl 85 
		faceangles 0 210 0 gototime
		trigger loper1 run
	}

	trigger attack
	{
		trigger loper1 attack1_if
		trigger loper1 attack2_if
		trigger loper1 attack3_if
		trigger loper1 attack4_if
	}

	trigger attack1_if
	{
		accum 0 abort_if_not_equal 1
		trigger loper1 attack1
	}
	
	trigger attack2_if
	{
		accum 0 abort_if_not_equal 2
		trigger loper1 attack2
	}
	
	trigger attack3_if
	{
		accum 0 abort_if_not_equal 3
		trigger loper1 attack3
	}
	
	trigger attack4_if
	{
		accum 0 abort_if_not_equal 4
		trigger loper1 attack4
	}

	trigger attack_routine
	{
		gotomarker loper1_spline5 500  
		faceangles 0 -180 0 gototime //look at player at the window
		playanim 206 213 rate 14 //setup before charge
		alertentity loper1_elec2

		gotomarker loper1_spline7 500 accel 
		playanim 214 226 rate 20 //frenzy attack
		alertentity loper1_glass
		alertentity loper1_moan
		
		gotomarker loper1_spline7_ctrl 300 
		faceangles 0 0 0 50
		playanim 227 236 rate 20 //roll-out

		accum 0 set 4
		playanim 91 102 looping forever 15 //walking
		trigger loper1 run
	}

	trigger attack1
	{
		gotomarker loper1_spline1_attack 500 wait 
		trigger loper1 attack_routine
	}

	trigger attack2
	{
		gotomarker loper1_spline2_attack 500 wait 
		trigger loper1 attack_routine
	}

	trigger attack3
	{
		gotomarker loper1_spline3_attack 500 wait 
		trigger loper1 attack_routine
	}

	trigger attack4
	{
		gotomarker loper1_spline4_attack 500 wait 
		trigger loper1 attack_routine
	}
}


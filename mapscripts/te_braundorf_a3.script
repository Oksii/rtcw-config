bell
{
	trigger ring
	{
		accum 0 set 1
		faceangles 18 0 0 500
		trigger bell_ball ring
		alertentity bell_sound
		wait 20
		trigger bell ring_continue
	}

	trigger ring_continue
	{
		accum 0 inc 1
		faceangles -18 0 0 1000
		alertentity bell_sound
		wait 20 
		faceangles 18 0 0 1000
		alertentity bell_sound
		wait 20

		trigger bell maybe_stop
		accum 0 abort_if_equal 7
		trigger bell ring_continue
	}

	trigger maybe_stop
	{
		accum 0 abort_if_not_equal 7
		trigger bell_ball stop
		trigger bell stop
	}

	trigger stop
	{
		faceangles -15 0 0 1000
		wait 20
		faceangles 12 0 0 1000
		wait 20
		faceangles -9 0 0 1000
		wait 20
		faceangles 6 0 0 1000
		wait 20
		faceangles -3 0 0 1000
		wait 20
		faceangles 0 0 0 1000
	}
}


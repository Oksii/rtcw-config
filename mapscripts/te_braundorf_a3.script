side_gate_trigger1
{
	spawn
	{
			accum 6 set 1
	}

	trigger side_gate_lever1up
	{
			accum 6 abort_if_not_equal 1
			trigger side_gate_lever1 up
			trigger side_gate_lever2 up
			alertentity light_green
			accum 6 set 0
			wait 15000
			alertentity light_red
			alertentity light_green
			trigger side_gate_trigger2 setaccum0
	}

	trigger setaccum0
	{
			accum 6 set 0
	}

	trigger setaccum1
	{
			accum 6 set 1
	}
}

side_gate_trigger2
{
	spawn
	{
			accum 6 set 1
	}

	trigger side_gate_lever1down
	{
			accum 6 abort_if_not_equal 0
			trigger side_gate_lever1 down
			trigger side_gate_lever2 down
			alertentity light_green
			accum 6 set 1
			wait 15000
			alertentity light_red
			alertentity light_green
			trigger side_gate_trigger1 setaccum1
	}

	trigger setaccum0
	{
			accum 6 set 0
	}

	trigger setaccum1
	{
			accum 6 set 1
	}
}


game_manager
{
	spawn
	{
		wm_mapdescription "Allied Intelligence have discovered a weapon research bunker outside of the town of Braundorf. It is the Allies mission to blow up the gas and electricity control systems, causing overloads which will destroy the facility."

		wm_axis_respawntime		30
		wm_allied_respawntime	25

		wm_number_of_objectives	6
		wm_set_round_timelimit	8

		wm_set_objective_status		1	0
		wm_set_objective_status		2	0
		wm_set_objective_status		3	0
		wm_set_objective_status		4	-1
		wm_set_objective_status		5	-1
		wm_set_objective_status		6	0	
		
		wm_objective_axis_desc		1	"Primary Objective:**Defend the Main Gate."
		wm_objective_axis_desc		2	"Primary Objective:**Defend the Bunker Controls."
		wm_objective_axis_desc		3	"Secondary Objective:**Defend the Side Door."
		wm_objective_axis_desc		4	"Secondary Objective:**Hold the Factory District."
		wm_objective_axis_desc		5	"Secondary Objective:**Hold the Side Entrance."		
		wm_objective_axis_desc		6	"Secondary Objective:**Keep the Side Gate closed."
		
		wm_objective_allied_desc	1	"Primary Objective:**Destroy the Main Gate."
		wm_objective_allied_desc	2	"Primary Objective:**Destroy the Bunker Controls."
		wm_objective_allied_desc	3	"Secondary Objective:**Destroy the Side Door."
		wm_objective_allied_desc	4	"Secondary Objective:**Capture the Factory District."
		wm_objective_allied_desc	5	"Secondary Objective:**Capture the Side Entrance."
		wm_objective_allied_desc	6	"Secondary Objective:**Open the Side Gate."

		wm_objective_image			1	"objectives/te_braundorf/te_braundorf_1.jpg"			
		wm_objective_image			2	"objectives/te_braundorf/te_braundorf_2.jpg"	
		wm_objective_image			3	"objectives/te_braundorf/te_braundorf_3.jpg"	
		wm_objective_image			4	"objectives/te_braundorf/te_braundorf_4.jpg"
		wm_objective_image			5	"objectives/te_braundorf/te_braundorf_5.jpg"	
		wm_objective_image			6	"objectives/te_braundorf/te_braundorf_6.jpg"			
		wm_overview_image				"levelshots/te_braundorf.jpg"

		wm_objective_short_axis_desc	1	"Defend the Main Gate"
		wm_objective_short_axis_desc	2	"Defend the Bunker Controls"
		wm_objective_short_axis_desc	3	"Defend the Side Door"
		wm_objective_short_axis_desc	4	"Hold the Factory District"
		wm_objective_short_axis_desc	5	"Hold the Side Entrance"
		wm_objective_short_axis_desc	6	"Keep the Side Gate closed"

		wm_objective_short_allied_desc	1	"Destroy the Main Gate"
		wm_objective_short_allied_desc	2	"Destroy the Bunker Controls"
		wm_objective_short_allied_desc	3	"Destroy the Side Door"
		wm_objective_short_allied_desc	4	"Capture the Factory District"
		wm_objective_short_allied_desc	5	"Capture the Side Entrance"
		wm_objective_short_allied_desc	6	"Open the Side Gate"

		wm_set_defending_team	0

		wm_setwinner 0

		accum 1 set 0 // main_gate 
		accum 2 set 0 // bunker_controls 
		accum 3 set 0 // allied_spawn
		accum 4 set 0 // factory_flag 
		accum 5 set 0 // side_flag 
	}

	trigger main_gate_destroyed
	{
		wm_set_objective_status		1	1
		wm_announce	"Allies have destroyed the Main Gate!"
		accum 1 set 1 
		
		accum 4 abort_if_not_equal 1
		trigger factory_flag kill
	}
	
	trigger bunker_controls_destroyed
	{
		wm_set_objective_status		2	1
		wm_announce	"Allies have destroyed the Bunker Controls!"
		accum 2 set 1
		trigger game_manager checkgame
	}

	trigger side_door_destroyed
	{
		wm_set_objective_status		3	1
		wm_announce	"Allies have destroyed the Side Door!"
	}
	
	trigger factory_flag_axis
	{
		wm_set_objective_status		4	0
		wm_announce	"Axis capture the Factory District!"
		accum 4 set 0
		
		accum 3 abort_if_not_equal 1
		alertentity allied_spawn
	}

	trigger factory_flag_allies
	{
		wm_set_objective_status		4	1
		wm_announce	"Allies capture the Factory District!"
		accum 4 set 1
		
		alertentity allied_spawn
		accum 3 set 1
		
	    wait 50		
		accum 1 abort_if_not_equal 1
		trigger factory_flag kill		
	}
	
	trigger side_flag_axis
	{
		wm_set_objective_status		5	-1
		wm_announce	"Axis reclaim the Side Entrance!"
	}

	trigger side_flag_allies
	{
		wm_set_objective_status		5	1
		wm_announce	"Allies capture the Side Entrance!"
	}
	
	trigger gate_close
	{
		wm_set_objective_status		6	0
		wm_announce	"The Side Gate is closing!"
	}
	
	trigger gate_open
	{
		wm_set_objective_status		6	1
		wm_announce	"The Side Gate is opening!"
	}
	
	trigger checkgame
	{
		accum 2 abort_if_not_equal 1
		wm_setwinner 1
		wait 500
		wm_endround
	}
}

